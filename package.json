{
  "name": "blog-api-rest",
  "version": "0.1.0",
  "description": "blog api rest",
  "main": "src/server.ts",
  "repository": "https://github.com/damiangamracy/blog-api-rest.git",
  "author": "Damian Gamracy <damiangamracy@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc",
    "start": "ts-node-dev src/server.ts",
    "demo": "node dist/server.js",
    "format": "run-p format:*",
    "format:ts": "prettier **/*.ts --write",
    "lint": "run-p lint:*",
    "lint:ts": "eslint **/*.ts --fix",
    "fix": "run-p fix:*",
    "fix:ts": "run-s format:ts lint:ts",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.17.0",
    "@types/node": "^12.0.8",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.5.2"
  }
}
